for number in range(1, 7):
# Variable for user guess    
    guess = int(input("What is your guess? "))
    if guess == numbers:
        print("Your answer is correct")
        break
    elif guess < numbers:
        print("That guess is too low")
    elif guess > numbers:
        print("That guess is too high")
    else:
        coninue
    print(f"I am sorry {name}, You've used all your guesses. The number you were looking for was {numbers}.")
